peSY.commemoration.text = "On this day, Monday after Pentecost, we celebrate the all-holy, life-giving, and almighty Spirit, God, one of the Trinity, same in honor, essence, and glory with the Father and the Son."
peSY.commemoration.couplet1.title = //titles_en_US_duvall.Stichoi
peSY.commemoration.couplet1.verse1 = "Everything that breathes, glorify the Lord's Spirit,"
peSY.commemoration.couplet1.verse2 = "through whom gone is the impudence of evil spirits."
peSY.commemoration.narration.p01 = "[paragraph 01]"
peSY.commemoration.narration.p02 = "[paragraph 02]"
peSY.commemoration.narration.p03 = "[paragraph 03]"
peSY.commemoration.narration.p04 = "[paragraph 04]"
peSY.commemoration.narration.p05 = "[paragraph 05]"
peSY.commemoration.narration.p06 = "[paragraph 06]"
peSY.commemoration.narration.p07 = "[paragraph 07]"
peSY.lastline = "By the coming of Your Holy Spirit, by the intercessions of Your Apostles, O Christ God, have mercy on us. Amen."

